<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>html/form.html</welcome-file>
    </welcome-file-list>
    
    <servlet-mapping>
        <servlet-name>Sss_Header</servlet-name>
        <url-pattern>/Sss_Header</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_Header</servlet-name>
        <servlet-class>servlet.Sss_Header</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss_XML</servlet-name>
        <url-pattern>/Sss_XML</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_XML</servlet-name>
        <servlet-class>servlet.Sss_XML</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss_JSON</servlet-name>
        <url-pattern>/Sss_JSON</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_JSON</servlet-name>
        <servlet-class>servlet.Sss_JSON</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss_Header_sync</servlet-name>
        <url-pattern>/Sss_Header_sync</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_Header_sync</servlet-name>
        <servlet-class>servlet.Sss_Header_sync</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss_XML_sync</servlet-name>
        <url-pattern>/Sss_XML_sync</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_XML_sync</servlet-name>
        <servlet-class>servlet.Sss_XML_sync</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss_JSON_sync</servlet-name>
        <url-pattern>/Sss_JSON_sync</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_JSON_sync</servlet-name>
        <servlet-class>servlet.Sss_JSON_sync</servlet-class>
    </servlet>
</web-app>